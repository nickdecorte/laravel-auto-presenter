{
    "name": "nickdecorte/laravel-auto-presenter",
    "description": "A system for auto-decorating models with presenter objects.",
    "keywords": ["lpm", "laravel", "eloquent", "presenter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn McCool",
            "email": "shawn@mccool.email",
            "homepage": "https://github.com/ShawnMcCool"
        },
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk",
            "homepage": "https://github.com/GrahamCampbell"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0 || ^8.1",
        "illuminate/container": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/contracts": "^5.5 || ^6.0 || ^7.0 || ^8.0|| ^9.0 || ^10.0",
        "illuminate/events": "^5.5 || ^6.0 || ^7.0 || ^8.0|| ^9.0 || ^10.0",
        "illuminate/pagination": "^5.5 || ^6.0 || ^7.0 || ^8.0|| ^9.0 || ^10.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0|| ^9.0 || ^10.0",
        "illuminate/view": "^5.5 || ^6.0 || ^7.0 || ^8.0|| ^9.0 || ^10.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.4 || ^3.0",
        "graham-campbell/testbench": "^5.4",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^6.5.14 || ^7.5.20 || ^8.5.8 || ^9.3.7"
    },
    "autoload": {
        "psr-4": {
            "McCool\\LaravelAutoPresenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McCool\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.7-dev"
        },
        "laravel": {
            "providers": [
                "McCool\\LaravelAutoPresenter\\AutoPresenterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
